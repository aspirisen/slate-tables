		"slate-hyperscript": "^0.13.8",
		"slate-react": "^0.22.8",
		"standard-version": "^6.0.1"
	},
	"scripts": {
		"test": "NODE_ENV=test jest --roots --roots ./tests",
		"start": "parcel examples/index.html",
		"release": "standard-version",
		"build": "rm -rf ./dist; NODE_ENV=production babel lib --out-dir dist",
		"prepack": "npm run build"
	},
	"jest": {
		"notify": false,
		"globals": {
			"__DEV__": false,
			"__PROD__": true,
			"__TEST__": true
		},
		"setupFilesAfterEnv": [
			"<rootDir>/jest.setup.js"
		],
		"testPathIgnorePatterns": [
			"/node_modules/",
			"/fixtures/"
		],
		"moduleFileExtensions": [
			"js",
			"jsx",
			"json"
		],
		"moduleDirectories": [
			"node_modules",
			"<rootDir>"
		],
		"roots": [
			"<rootDir>"
		],
		"testMatch": [
			"<rootDir>/tests/**/*.js",
			"<rootDir>/tests/**/*.jsx"
		],
		"transform": {
			"^.+\\.jsx?$": "babel-jest"
		},
		"watchPlugins": [
			"jest-watch-typeahead/filename",
			"jest-watch-typeahead/testname"
		]
	},
	"babel": {
		"presets": [
			[
				"@babel/preset-env"
			]
		],
		"plugins": [
			"@babel/plugin-proposal-class-properties"
		]
	},
	"repository": "git@gitlab.com:elium/product/slate-tables.git",
	"author": "Quentin GÃ©rome <q.gerome@whatever-company.com>",
	"license": "MIT"
}
