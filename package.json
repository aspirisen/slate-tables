{
	"name": "@whatever-company/slate-tables",
	"version": "1.1.3",
	"description": "A Slate plugin to handle table edition",
	"main": "./dist/index.js",
	"files": [
		"dist",
		"lib",
		"*.md"
	],
	"peerDependencies": {
		"react": "^18.1.0",
		"react-dom": "^18.1.0",
		"slate": "^0.78.0",
		"slate-react": "^0.79.0"
	},
	"dependencies": {
		"@types/lodash.findlast": "^4.6.7",
		"core-js": "3.22.5",
		"is-hotkey": "0.2.0",
		"lodash.findlast": "^4.6.0"
	},
	"devDependencies": {
		"@babel/cli": "7.17.10",
		"@babel/core": "7.17.12",
		"@babel/eslint-parser": "^7.17.0",
		"@babel/plugin-proposal-class-properties": "7.17.12",
		"@babel/preset-env": "7.17.12",
		"@babel/preset-react": "7.17.12",
		"@types/react": "^18.0.9",
		"@types/react-dom": "^18.0.4",
		"babel-jest": "28.1.0",
		"eslint": "8.15.0",
		"eslint-config-prettier": "8.5.0",
		"eslint-config-standard": "17.0.0",
		"eslint-config-standard-react": "11.0.1",
		"eslint-plugin-babel": "5.3.1",
		"eslint-plugin-import": "2.26.0",
		"eslint-plugin-node": "11.1.0",
		"eslint-plugin-prettier": "4.0.0",
		"eslint-plugin-promise": "6.0.0",
		"eslint-plugin-react": "7.29.4",
		"eslint-plugin-react-hooks": "4.5.0",
		"eslint-plugin-standard": "4.1.0",
		"immutable": "4.0.0",
		"jest": "28.1.0",
		"jest-diff": "28.1.0",
		"jest-watch-typeahead": "1.1.0",
		"lodash.isequal": "4.5.0",
		"parcel": "^2.5.0",
		"prettier": "2.6.2",
		"react": "18.1.0",
		"react-dom": "18.1.0",
		"react-test-renderer": "18.1.0",
		"slate": "^0.78.0",
		"slate-hyperscript": "^0.77.0",
		"slate-react": "^0.79.0",
		"standard-version": "9.5.0",
		"typescript": "^4.6.4"
	},
	"scripts": {
		"test": "NODE_ENV=test jest --roots --roots ./tests",
		"start": "parcel examples/index.html",
		"build-examples": "rm -rf ./dist-examples;parcel build examples/index.html -d dist-examples --public-url /slate-tables",
		"release": "standard-version",
		"build": "rm -rf ./dist; NODE_ENV=production babel lib --out-dir dist",
		"prepack": "npm run build"
	},
	"jest": {
		"notify": false,
		"globals": {
			"__DEV__": false,
			"__PROD__": true,
			"__TEST__": true
		},
		"setupFilesAfterEnv": [
			"<rootDir>/jest.setup.js"
		],
		"testPathIgnorePatterns": [
			"/node_modules/",
			"/fixtures/"
		],
		"moduleFileExtensions": [
			"js",
			"jsx",
			"json"
		],
		"moduleDirectories": [
			"node_modules",
			"<rootDir>"
		],
		"roots": [
			"<rootDir>"
		],
		"testMatch": [
			"<rootDir>/tests/**/*.js",
			"<rootDir>/tests/**/*.jsx"
		],
		"transform": {
			"^.+\\.jsx?$": "babel-jest"
		},
		"watchPlugins": [
			"jest-watch-typeahead/filename",
			"jest-watch-typeahead/testname"
		]
	},
	"babel": {
		"presets": [
			[
				"@babel/preset-env"
			]
		],
		"plugins": [
			"@babel/plugin-proposal-class-properties"
		]
	},
	"repository": "git@github.com:whatever-company/slate-tables.git",
	"author": "Elium <info@elium.com>",
	"license": "MIT"
}
